  "devDependencies": {
    "@dbos-inc/dbos-cloud": "^1.15.9",
    "@dbos-inc/eslint-plugin": "^3.1.0",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^2.0.14",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@dbos-inc/dbos-sdk": "^1.15.9"
  }
}
