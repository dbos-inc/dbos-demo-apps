# AWS ECR Configuration
# Your AWS ECR repository URL (e.g., ACCOUNT_ID.dkr.ecr.REGION.amazonaws.com/REPO_NAME)
ECR_REPO=

# AWS Region
# Your AWS region (e.g., us-east-1)
AWS_REGION=

# AWS Load Balancer Subnets
# Comma-separated list of subnet IDs for your AWS Load Balancer
AWS_LB_SUBNETS=

# PostgreSQL Configuration
# A secure password for PostgreSQL
POSTGRES_PASSWORD=

# Kubernetes Configuration
# The Kubernetes namespace where you'll deploy (default: default)
KUBERNETES_NAMESPACE=default

# DBOS Application Configuration
# The name for your DBOS application (default: dbos-app)
DBOS_APP_NAME=dbos-app

# Docker Image Tag
# The tag to use for the Docker image (default: latest)
IMAGE_TAG=latest
